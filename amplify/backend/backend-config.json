			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "sadevotegraphqlapi",
							"apiKeyExpirationDays": "300"
						}
					}
				}
			}
		}
