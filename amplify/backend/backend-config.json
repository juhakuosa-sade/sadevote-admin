{
    "hosting": {
        "amplifyhosting": {
            "service": "amplifyhosting",
            "providerPlugin": "awscloudformation",
            "type": "manual"
        },
        "S3AndCloudFront": {
            "service": "S3AndCloudFront",
            "providerPlugin": "awscloudformation"
        }
    },
    "auth": {
        "sadevoteadmina21d98fd": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [],
            "customAuth": false
        }
    },
    "storage": {
        "dynamovoter": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "sadevoteVoter": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "dynamovoter",
                    "attributes": [
                        "Name",
                        "Arn"
                    ],
                    "attributeEnvMap": {
                        "Name": "TABLE_NAME",
                        "Arn": "TABLE_ARN"
                    }
                }
            ]
        }
    },
    "api": {
        "sadevoterestapi": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "sadevoteVoter",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        },
        "sadevoteGraphQLapi": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "output": {
                "authConfig": {
                    "additionalAuthenticationProviders": [],
                    "defaultAuthentication": {
                        "authenticationType": "API_KEY",
                        "apiKeyConfig": {
                            "description": "sadevoteGraphQLapiKey",
                            "apiKeyExpirationDays": 7
                        }
                    }
                }
            }
        }
    }
}